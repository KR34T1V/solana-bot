# Solana Network Configuration
SOLANA_CLUSTER=devnet                                # Options: devnet, testnet, mainnet-beta
RPC_ENDPOINT=https://api.devnet.solana.com          # Solana RPC endpoint

# Wallet Configuration
# Your wallet private key (Keep this secret and never commit the actual .env file!)
PRIVATE_KEY=                                        # Base58 encoded private key

# Market Configuration
# The address of the market you want to trade on (if empty, will use default for the cluster)
# Default devnet SOL/USDC market: 9wFFyRfZBsuAha4YcuxcXLKwMxJR43S7fPfQLusDBzvT
MARKET_ADDRESS=

# Trading Parameters
MIN_SPREAD_PERCENTAGE=1.5     # Minimum spread to consider for trading
MAX_TRADE_SIZE_SOL=1.0       # Maximum size of each trade in SOL
MIN_SOL_BALANCE=0.1          # Minimum SOL balance required to trade

# Dry Run Configuration
DRY_RUN=true                 # Set to false for real trading
DRY_RUN_INITIAL_SOL=10.0     # Initial SOL balance for dry run testing

# CoinGecko API Configuration
COINGECKO_API_KEY=your_api_key_here